#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/matehdz/Documents/Software Projects/BeautySync/node_modules/.pnpm/lefthook@2.0.13/node_modules/lefthook/bin/node_modules:/Users/matehdz/Documents/Software Projects/BeautySync/node_modules/.pnpm/lefthook@2.0.13/node_modules/lefthook/node_modules:/Users/matehdz/Documents/Software Projects/BeautySync/node_modules/.pnpm/lefthook@2.0.13/node_modules:/Users/matehdz/Documents/Software Projects/BeautySync/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/matehdz/Documents/Software Projects/BeautySync/node_modules/.pnpm/lefthook@2.0.13/node_modules/lefthook/bin/node_modules:/Users/matehdz/Documents/Software Projects/BeautySync/node_modules/.pnpm/lefthook@2.0.13/node_modules/lefthook/node_modules:/Users/matehdz/Documents/Software Projects/BeautySync/node_modules/.pnpm/lefthook@2.0.13/node_modules:/Users/matehdz/Documents/Software Projects/BeautySync/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/lefthook@2.0.13/node_modules/lefthook/bin/index.js" "$@"
else
  exec node  "$basedir/../.pnpm/lefthook@2.0.13/node_modules/lefthook/bin/index.js" "$@"
fi
